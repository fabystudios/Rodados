{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/.*)(?!_next/.*)(?!favicon.ico)(?!vite.svg)(?!.*\\.).*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}